{% extends 'front/base.html.twig' %}

{% block title %}Constat{% endblock %}

{% block body %}
    <h1>Constat</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ constat.id }}</td>
            </tr>
            <tr>
                <th>TypeDAccident</th>
                <td>{{ constat.TypeDAccident }}</td>
            </tr>
            <tr>
                <th>LieuDAccident</th>
                <td>{{ constat.LieuDAccident }}</td>
            </tr>
            <tr>
                <th>VehiculeA</th>
                <td>{{ constat.vehiculeA }}</td>
            </tr>
            <tr>
                <th>VehiculeB</th>
                <td>{{ constat.VehiculeB }}</td>
            </tr>
            <tr>
                <th>ImmatriculeA</th>
                <td>{{ constat.immatriculeA }}</td>
            </tr>
            <tr>
                <th>ImmatriculeB</th>
                <td>{{ constat.immatriculeB }}</td>
            </tr>
            <tr>
                <th>DateDAccident</th>
                <td>{{ constat.DateDAccident ? constat.DateDAccident|date('Y-m-d') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('admin_constat_index') }}"><button type="button" class="btn btn-outline-primary">back to list</a>

    <a href="{{ path('admin_constat_edit', {'id': constat.id}) }}"><button type="button" class="btn btn-outline-primary">edit</a>

    {{ include('constat/_delete_form.html.twig') }}
{% endblock %}
